@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<h2>Access Token : </h2>
<p>@ViewBag.AccessToken</p>

<h2>Id Token : </h2>
<p>@ViewBag.IdToken</p>

<h2>Refresh Token : </h2>
<p>@ViewBag.RefreshToken</p>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>
